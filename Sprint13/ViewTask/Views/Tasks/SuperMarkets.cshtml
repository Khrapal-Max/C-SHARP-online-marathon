@{
    ViewData["Title"] = "SuperMarkets";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="text-left">
    <h1>Our Supermarkets</h1>
    <p>
        <h3>There are 5 markets:</h3>
</div>
<div>
    @foreach (var item in ViewBag.Markets)
    {
        <p>@item</p>
    }
    <p>Available Stores: @ViewBag.Markets.Count</p>
</div>



